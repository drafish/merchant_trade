{
  "env": {
      "node": true,
      "es6": true,
      "es2020": true
  },
  "extends": [
      "eslint:recommended"
  ],
  "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
  },
  "rules": {
      "indent": ["error", 2],
      "quotes": ["error", "single"],
      "semi": ["error", "never"],
      "no-unused-vars": "warn",
      "no-undef": "error"
  }
}

